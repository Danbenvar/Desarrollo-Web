        <script>
            
            class Hospital{

                constructor(nombreHospital, ciudad, numPacientes){

                    this.nombreHospital = nombreHospital;
                    this.ciudad = ciudad;
                    this.numPacientes = new Array();

                    for(let i = 0; i < numPacientes; i++){

                    this.numPacientes[i] = new Paciente(i, i, i);
                    }
                }
            }


            class Paciente{

                constructor(DNI, nombrePaciente, enfermedad){

                    this.DNI = DNI;
                    this.nombrePaciente = nombrePaciente;
                    this.enfermedad = enfermedad;
                }
            }



                alta(codigo)

                    var borrar;

                    for(let i = 0; i < numPacientes; i++){

                        if(this.numPacientes[i].find(codigo)){

                            borrar = numPacientes.splice()
                        }
                    }
                
            
        </script>
