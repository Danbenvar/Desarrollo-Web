<!DOCTYPE html>
<html>

<head>
    <title>Javascript ejercicio 5</title>
    <meta charset="UTF-8">
</head>

<body>

    <script>
        let numero1;
        let numero2;
        let numero3;
        let numero4;
        num1 = prompt('Ingrese el primer número:');
        num2 = prompt('Ingrese el segundo número:');
        num3 = prompt('Ingrese el tercer número:');
        num4 = prompt('Ingrese el cuarto número:');
        let suma;
        suma = parseInt(numero1) + parseInt(numero2);
        let producto;
        producto = parseInt(numero3) * parseInt(numero4);
        document.write('La suma de los dos primeros valores es:');
        document.write(suma);
        document.write('<br>');
        document.write('El producto del tercer y cuarto valor es:');
        document.write(producto);
    </script>

</body>

</html>
